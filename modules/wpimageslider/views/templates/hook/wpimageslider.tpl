{if $wpimageslider.slides}    <div class="row parent {if isset($wpimageslider.wpis_width) && $wpimageslider.wpis_width == 'fullwidth'}fullwidth-row{/if}">        <div id="wpimageslider" class="column col-12-12">            <ul class="slides">                {foreach $wpimageslider.slides as $slide name=slider}                     <li>                         {if (isset($slide->link) && $slide->link != '')}                             <a href="{$slide->link}"                                {if (isset($slide->html) && $slide->html != '')} title="{$slide->html|strip_tags}"{/if}                                {if ($slide->open_in_new == 1)} target="_blank"{/if}>                         {/if}                        <img src="{$slide->slideImage}"                             {if (isset($slide->html) && $slide->html != '')} alt="{$slide->html|strip_tags}"{/if} />                         {*                        {if (isset($slide->html) && $slide->html != '')}                            <div class="flex-caption {$slide->html_position}">                                <div class="flex-caption-html-content">                                    {$slide->html}                                </div>                            </div>                        {/if}                        *}                         {if (isset($slide->link) && $slide->link != '')}                             </a>                         {/if}                    </li>                                    {/foreach}            </ul>        </div>    </div>    {addJsDef wpis_effect=$wpimageslider.wpis_effect}    {addJsDef wpis_autoscrolldelay=$wpimageslider.wpis_autoscrolldelay}    {addJsDef wpis_autoscrollspeed=$wpimageslider.wpis_autoscrollspeed}    {if ($wpimageslider.wpis_directionalnav)}        {addJsDef wpis_directionalnav=true}    {else}        {addJsDef wpis_directionalnav=false}    {/if}    {if ($wpimageslider.wpis_controlnav)}        {addJsDef wpis_controlnav=true}    {else}        {addJsDef wpis_controlnav=false}    {/if}    {if ($wpimageslider.wpis_autoscroll)}        {addJsDef wpis_autoscroll=true}    {else}        {addJsDef wpis_autoscroll=false}    {/if}    {if ($wpimageslider.wpis_pauseonhover)}        {addJsDef wpis_pauseonhover=true}    {else}        {addJsDef wpis_pauseonhover=false}    {/if}{/if}